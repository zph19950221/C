#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int k;
    int l = 0;
    int j = 0;
    for(i = 0; i < 100;i += 2)
    {
        j+=i;
        printf("%d + %d = %d\n",i,j,j);
        printf("%d\n",j);
    }
    printf("ºÍÊÇ1£º%d",j);
    for(k = 0; k < 100; k++)
    {
        if(k % 2 != 0)
            continue;
        l +=k;
    }
    printf("ºÍÊÇ2£º%d",l);
    return 0;
}
