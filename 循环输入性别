// 循环输入性别.cpp: 定义控制台应用程序的入口点。
//

#include "stdafx.h"


int main()
{
	int count = 0;
	char sex;
	for(;;)
	{
		printf("请输入性别字符：\n");
		scanf_s("%c",&sex);
		fflush(stdin);
		if (sex == 'm')
		{
			count++;
		}
		else
		{
			printf("输入错误");
			break;
		}
		
	}
	
	printf("输入的正确次数是：%d", count);
    return 0;
}

